{
	"name": "obsidian-handwrite-ocr",
	"version": "1.0.0",
	"description": "Convert handwritten notes from images and PDFs into Markdown using Gemini AI",
	"main": "main.js",
	"scripts": {
		"dev": "npm run build && npm run copy-files",
		"dev:watch": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"copy-files": "node copy-files.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "plugin", "handwriting", "ocr", "gemini", "ai"],
	"author": "Callum Alpass",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/callumalpass/handwrite-obsidian"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@google/genai": "^1.11.0"
	}
}
